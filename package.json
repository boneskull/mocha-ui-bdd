{
  "name": "mocha-ui-bdd",
  "version": "0.0.0",
  "description": "BDD-style user interface for Mocha",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run lint",
    "build": "npm run build-node",
    "build-node": "rimraf lib; babel src --out-dir lib",
    "lint": "eslint src/ test/ .*.js",
    "prepublish": "npm run build-node",
    "test-e2e": "npm run test-node-unit; npm run test-browser-e2e",
    "test-node-e2e": "babel-node node_modules/.bin/_mocha --require test/e2e/fixture.js --recursive test/e2e/",
    "test-browser-e2e": "karma start .karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boneskull/mocha-ui-bdd.git"
  },
  "author": {
    "name": "Christopher Hiller",
    "email": "boneskull@boneskull.com",
    "url": "https://mochajs.org"
  },
  "engines": {
    "node": ">=0.10.0",
    "npm": ">=1.7.4"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/boneskull/mocha-ui-bdd/issues"
  },
  "homepage": "https://github.com/boneskull/mocha-ui-bdd#readme",
  "files": [
    "lib/"
  ],
  "devDependencies": {
    "babel-cli": "^6.3.13",
    "babel-core": "^6.1.2",
    "babel-eslint": "^4.1.5",
    "babel-istanbul": "^0.5.9",
    "babel-preset-es2015": "^6.1.2",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "browserify-istanbul": "^0.2.1",
    "chai": "^3.4.0",
    "chai-as-promised": "^5.1.0",
    "eslint": "^1.10.3",
    "eslint-config-semistandard": "^5.0.0",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-lodash3": "^0.4.0",
    "eslint-plugin-mocha-only": "0.0.3",
    "eslint-plugin-no-class": "^0.1.0",
    "eslint-plugin-prototype-chain": "^0.1.0",
    "eslint-plugin-standard": "^1.3.1",
    "istanbul": "^0.4.1",
    "js-yaml": "^3.4.3",
    "karma": "^0.13.15",
    "karma-browserify": "^4.4.2",
    "karma-cli": "^0.1.1",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.3",
    "karma-phantomjs2-launcher": "^0.3.2",
    "minifyify": "^7.1.0",
    "mocha": "^2.3.4",
    "mocha3": "file:../mocha3",
    "phantomjs2-ext": "^0.1.0",
    "pre-commit": "^1.1.2",
    "publish-please": "^1.1.0",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
